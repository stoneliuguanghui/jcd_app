  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.name %></td></br>
        <td><%= image_tag(image.avatar_url,:style=>"width:500px;height:500px;")%></td></br>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td></br>
      </tr>
    <% end %>
  </tbody>